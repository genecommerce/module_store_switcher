{
    "name": "gene/module-store-switcher",
    "description": "Magento 2.x module to add alternative store switcher functionality to the core implementation.",
    "version": "0.5.2",
    "keywords": ["Store Switcher", "Gene", "Magento 2"],
    "require": {
        "php": "~7.4.0||~8.1.0",
        "magento/framework": "*",
        "magento/magento-coding-standard": "*",
        "phpunit/phpunit": "^9.1",
	    "tddwizard/magento2-fixtures": "^1.1.2",
        "magento/module-store": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "*",
        "bitexpert/phpstan-magento": "^0.23"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Gene\\StoreSwitcher\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    }
}
